{
  "name": "fitness",
  "version": "1.0.0",
  "description": "피트니스 회원 관리 프로그램",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:webpack\" \"npm run dev:electron\"",
    "dev:webpack": "webpack serve --config webpack.config.js --mode development",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "build": "webpack --config webpack.config.js --mode production && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.11.5",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.9.1",
    "electron": "^28.1.4",
    "electron-builder": "^24.9.1",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.5.3",
    "postcss-loader": "^7.3.4",
    "prettier": "^3.2.4",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.1"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "better-sqlite3": "^9.3.0",
    "chart.js": "^4.4.1",
    "date-fns": "^4.1.0",
    "electron-log": "^5.0.3",
    "framer-motion": "^10.18.0",
    "fs-extra": "^11.2.0",
    "lucide-react": "^0.309.0",
    "node-cron": "^3.0.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "build": {
    "appId": "com.fitness.manager",
    "productName": "fitness",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    }
  }
}
